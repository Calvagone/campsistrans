[MAIN]
KG=THETA_KG * exp(ETA_KG)
KD0=THETA_KD0 * exp(ETA_KD0)
KD1=THETA_KD1 * exp(ETA_KD0)
IBASE=THETA_IBASE * exp(ETA_IBASE)
FADD=THETA_FADD
INIT_A1=IBASE * 1000
if (newind != 2) OCB=CB
if (newind != 2) OG=G
E0=OCB
E1=OG
OCB=CB
OG=G
DUMMY_EQ=0

[ODE]
d/dt(A_TUMOUR)=KG/1000 * A_TUMOUR - (KD0/1000 * E0 + KD1/100 * E1) * A_TUMOUR
F=A_TUMOUR
LLOQ=5
IPRED=A_TUMOUR
W=FADD
DUM=(LLOQ - IPRED)/W
DUM2=phi(DUM)
if (BQL == 1) F_FLAG=1
if (BQL == 1) Y=DUM2
IRES=IPRED - DV
IWRES=IRES/W

[INIT]
A_TUMOUR=INIT_A1

