[MAIN]
K12=THETA_K12
K21=0
if (TIME <= 16) K21=THETA_K21_1
if (TIME > 16) K21=THETA_K21_2
K23=0
if (TIME <= 16) K23=THETA_K23
if (TIME > 16) K23=THETA_K21_2
K32=THETA_K21_1*exp(ETA_K32)
FOR1=THETA_FOR1
FOR2=0
if (TIME <= 16) FOR2=THETA_FOR2
if (TIME > 16) FOR2=THETA_FOR1
BACK=THETA_BACK
CSS=0
if (TRT == 3) CSS=(125.0/3.0)*DOSR*FR/CLR
if (TRT == 4) CSS=(125.0/3.0)*DOSE*FR/CLR
if (TRT == 5) CSS=(250.0/3.0)*DOSR*FR/CLR
if (TRT == 6) CSS=(250.0/3.0)*DOSE*FR/CLR
EFFH=THETA_EFFH
EFFP=THETA_EFFP
EFFZ=THETA_EFFZ
EFFJ=THETA_EFFJ
CTY1=0
if (CTRY == 1) CTY1=THETA_CTY1
HAL=CSS*EFFH
PAL=CSS*EFFP
ZIP=CSS*EFFZ
JJ6=CSS*EFFJ
FEFF=0
if (TRT == 3) FEFF=HAL
if (TRT == 4) FEFF=PAL
if (TRT == 5) FEFF=ZIP
if (TRT == 6) FEFF=JJ6

[ODE]
KF12=K12*(FEFF + 1)*exp(-FOR1*t)
KB21=K21*(CTY1 + 1)*exp(-BACK*t)
KF23=K23*(FEFF + 1)*exp(-FOR2*t)
KB32=K32*(CTY1 + 1)*exp(-BACK*t)
TIM=t
d/dt(A_1)=-A_1*KF12 + A_2*KB21
d/dt(A_2)=A_1*KF12 - A_2*KB21 - A_2*KF23 + A_3*KB32
d/dt(A_3)=A_2*KF23 - A_3*KB32

[ERROR]
PABS=A_1
PMIL=A_2
PMOD=A_3
Y=1
if (CMT == 1 && DV == 1) Y=A_1
if (CMT == 2 && DV == 1) Y=A_2
if (CMT == 3 && DV == 1) Y=A_3
ABS=A_1
MIL=A_2
MOSE=A_3

