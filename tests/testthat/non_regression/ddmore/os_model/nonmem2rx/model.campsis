[MAIN]
LAM=THETA_LAM * exp(ETA_LAM)
SHP=THETA_SHP
BSLD0=THETA_BSLD0
BTSR=THETA_BTSR
BNWLS=THETA_BNWLS
BECOG=THETA_BECOG
INIT_A1=IBASE * 1000
MMBAS=IBASE * 1000
if (newind != 2) OCB=CB
if (newind != 2) OG=G
E0=OCB
E1=OG
OCB=CB
OG=G
if (newind != 2) ONWLS=NWLS
INWLS=ONWLS
ONWLS=NWLS
TVSLD0=70
NSLD0=SLD0/TVSLD0
IECOG=ECOG

[ODE]
d/dt(A_TUMOUR)=KG/1000 * A_TUMOUR - (KD0/1000 * E0 + KD1/100 * E1) * A_TUMOUR
TUM=A_TUMOUR
TSR=(TUM - MMBAS)/MMBAS
if (t == 0) WTS=0
if (t == 0) TM12=0
DEL=1e-06
d/dt(A_HDEATH)=LAM * SHP * (LAM * (t + DEL))^(SHP - 1) * exp(BSLD0 * NSLD0 + BTSR * WTS + BNWLS * INWLS + BECOG * IECOG)
F=A_TUMOUR

[INIT]
A_TUMOUR=INIT_A1

[ERROR]
DELX=1e-06
XTUM=A_TUMOUR
XTSR=(XTUM - MMBAS)/MMBAS
if (t == 0) XWTS=0
if (t == 0) XTM12=0
CHZ=A_HDEATH
SUR=exp(-CHZ)
HAZN=LAM * SHP * (LAM * (t + DELX))^(SHP - 1) * exp(BSLD0 * NSLD0 + BTSR * XWTS + BNWLS * INWLS + BECOG * IECOG)
if (FLG == 9 && nmevid == 0 && OSCENS == 1) IPRED=SUR
if (FLG == 9 && nmevid == 0 && OSCENS == 1) Y=IPRED

