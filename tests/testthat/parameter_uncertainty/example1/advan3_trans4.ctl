$THETA (0,20)    ; V2 
$THETA (0,4)     ; Q
$OMEGA 0.025 ; CL
$OMEGA 0.025 ; V1
;$OMEGA 0.025 ; V2
;$OMEGA 0.025 ; Q
$SIGMA 0.025 ; PROP

$EST METHOD = 1
$COV PRINT=E
